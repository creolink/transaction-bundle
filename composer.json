{
	"name" : "creolink/transaction-bundle",
	"type" : "symfony-bundle",
	"description" : "Transaction bundle for Symfony",
	"keywords" : [
		"transaction",
		"bundle",
		"symfony",
		"aop",
		"inneair",
		"tx"
	],
	"license" : "Apache-2.0",
	"authors" : [{
			"name" : "Inneair",
			"homepage" : "https://www.github.com/Inneair"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Inneair\\TransactionBundle\\Test\\" : "tests/",
			"Inneair\\TransactionBundle\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"jms/aop-bundle": "^1.3",
		"symfony/config": "^4.1",
		"symfony/dependency-injection": "^4.1",
		"symfony/doctrine-bridge": "^4.1",
		"symfony/http-kernel": "^4.1",
		"doctrine/orm": "^2.6",
		"psr/log": "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6.0",
		"php-coveralls/php-coveralls": "^2.1"
	},
	"config" : {
		"bin-dir" : "bin"
	},
	"minimum-stability" : "stable",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev"
		}
	}
}
